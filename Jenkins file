pipeline {
    agent any
    environment {
        DOTNET_CLI_HOME = "C:\\Program Files\\dotnet"
    }
     stages {
        stage('Checkout') {
            steps {
                // Checkout the code from the repository
                checkout scm
            }
        }
         stage('Build') {
            steps {
                script { 
                    dotnetBuild sdk: '.net sdk'
                }
            }
        }
